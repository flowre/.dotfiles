% Fonts
\renewcommand{\rm}{\textrm}
\renewcommand{\bf}{\textbf}
\renewcommand{\it}{\textit}
\renewcommand{\tt}{\texttt}
\renewcommand{\sc}{\textsc}
\newcommand{\ol}{\overline}
